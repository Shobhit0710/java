package palindrome ;

import java.util.Scanner;

public class check { 
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

System.out.println("enter the value of a");
int a=sc.nextInt();
int temp,rem,rev = 0;
temp=a;
while(a>0) {
	rem=a%10;
   	rev=(rev*10)+rem;
	a/=10;
	
	}
 if(temp==rev) {
	 System.out.println("a is a palindrome ");
	}
 else
	 System.out.println("not palindrome");
	}
}
