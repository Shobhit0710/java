package Tester;

class person{
String name;
float age;
  String getName() {
	  return name;
	   
   }
   void setName(String name) {
	  this.name=name ;
  }
  float getAge() {
		  return age;
		   
	   }
	   void setAge(float age) {
		  this.age=age ;
	  }
}
class person2{
String name2;
float age2;
  String getName2() {
	  return name2;
	   
   }
   void setName2(String name) {
	  this.name2=name2 ;
  }
  float getAge2() {
		  return age2;
		   
	   }
	   void setAge2(float age) {
		  this.age2=age2 ;
	  }
}
public class prac3 {
	public static void main(String[] args) {
		person p1= new person();
		p1.setName("shobhit");
		p1.setAge(23.5f);
		String name = p1.getName();
       float age = p1.getAge();
       System.out.println("Name of person 1 is"+ name);
       System.out.println("age of person 1 is"+ age);
		
       person2 p2= new person2();
		p2.setName2("shobhit sharma");
		p2.setAge2(24.5f);
		
		   String name2 = p2.getName2();
	       float age2 = p2.getAge2();
	       System.out.println("Name of person 2 is"+ name2);
	       System.out.println("age of person 2 is"+ age2);

		
		
	}
	

}
